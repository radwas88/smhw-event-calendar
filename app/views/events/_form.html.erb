<form id="event-form" method="post" onsubmit="return createCalendarEvent()">
	<fieldset>
		<legend>Create event</legend>
		<div>
			<label>Start date</label>
			<input type="text" name="start_date" class="date-input" required/>
		</div>
		<div>
			<label>End date</label>
			<input type="text" name="end_date" class="date-input" required/>
		</div>
		<div>
			<label>Title</label>
			<input type="text" name="title" required/>
		</div>		
	</fieldset>
	<button type="submit">Create</button>
</form>

<script type="text/javascript">
	function createCalendarEvent() {
		return false;
	}

	$(document).ready(function(){
		$('.date-input').datepicker();
	});
</script>